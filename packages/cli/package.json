{
  "name": "zplang-cli",
  "version": "0.0.34",
  "description": "Zaplang CLI",
  "author": "Alexandru Ghiura @ghalex",
  "bin": {
    "zplang-cli": "./bin/run"
  },
  "homepage": "https://github.com/zapant-com/zp-lang",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/zapant-com/zp-lang/tree/monorepo/packages/cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm lint",
    "prepack": "pnpm build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/zapant-com/zp-lang/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@oclif/core": "^3.26.0",
    "@oclif/plugin-help": "^6.0.20",
    "@oclif/plugin-plugins": "^5.0.1",
    "chalk": "^5.3.0",
    "shelljs": "^0.8.5",
    "signale": "^1.4.0",
    "zplang": "workspace:*"
  },
  "devDependencies": {
    "@oclif/test": "^3.2.8",
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.30",
    "@types/shelljs": "^0.8.15",
    "@types/signale": "^1.4.7",
    "chai": "^5.1.0",
    "mocha": "^10.4.0",
    "oclif": "^4.7.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  },
  "oclif": {
    "bin": "zplang-cli",
    "dirname": "zplang-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  }
}